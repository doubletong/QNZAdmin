                            input#fromDate.form-control(type='date', placeholder='请输入日期', aria-label='开始日期', aria-describedby="basic-addon1")
                    .col-md                   
                        .input-group
                            span#basic-addon2.input-group-text 到                       
                            input#toDate.form-control(type='date', placeholder='请输入日期', aria-label='结束日期', aria-describedby="basic-addon2")
